<odoo>
    <record id="view_project_expense_product_line_form" model="ir.ui.view">
        <field name="name">project.expense.request.product.line.form</field>
        <field name="model">project.expense.request.product.line</field>
        <field name="arch" type="xml">
            <form>
                <group string="qwwwwwwww">
                    <field name="product_id"/>
                    <field name="quantity"/>
                </group>
            </form>
        </field>
    </record>

    <record id="view_project_expense_product_line_list" model="ir.ui.view">
        <field name="name">project.expense.request.product.line.list</field>
        <field name="model">project.expense.request.product.line</field>
        <field name="arch" type="xml">
            <list editable="bottom">
                <field name="product_id"/>
                <field name="quantity"/>
            </list>
        </field>
    </record>
</odoo>
