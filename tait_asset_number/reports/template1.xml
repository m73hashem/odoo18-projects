<odoo>
<template id="asset_label_report_template">
  <t t-call="web.basic_layout">

    <style>
      @page {
        size: 6cm 4cm;
        
      }

      .label {
        width: 6cm;
        height: 4cm;
        font-family: Arial, sans-serif;
        
      }

      .label table {
        width: 100%;
        height: 100%;
        border-collapse: collapse;
      }

       .label tr {
                    height: 50%;
                }

      .label th, .label td {
        
        padding: 3px 5px;
        text-align: left;
        vertical-align: middle;
        border: 1px solid black;
      }

      .label th {
        background-color: #f0f0f0;
        font-weight: bold;
        width: 30%;
      }

      .label td {
        width: 70%;
        
      }
    </style>
     
    <t t-foreach="docs" t-as="o">
      <div class="label">
        <table>
          <tr >
            <th>Name:</th>
            <td><t t-esc="o.name"/></td>
          </tr>
          <tr>
            <th>Number:</th>
            <td><t t-esc="o.asset_sequence"/></td>
          </tr>
        </table>
      </div>
    </t>
     
  </t>
</template>
</odoo>