<odoo>
  <record id="label_4x6cm" model="report.paperformat">
    <field name="name">Label 4x6 cm</field>
    <field name="default" eval="False"/>
    <field name="format">custom</field>
    <field name="page_height">40</field>
    <field name="page_width">60</field>
    <field name="orientation">Portrait</field>
    <field name="margin_top">0</field>
    <field name="margin_bottom">0</field>
    <field name="margin_left">0</field>
    <field name="margin_right">0</field>
    <field name="header_line" eval="False"/>
    <field name="dpi">150</field>
  </record>

  <record id="label_6x10cm" model="report.paperformat">
    <field name="name">Label 6x10 cm</field>
    <field name="default" eval="False"/>
    <field name="format">custom</field>
    <field name="page_height">60</field>
    <field name="page_width">100</field>
    <field name="orientation">Portrait</field>
    <field name="margin_top">0</field>
    <field name="margin_bottom">0</field>
    <field name="margin_left">0</field>
    <field name="margin_right">0</field>
    <field name="header_line" eval="False"/>
    <field name="dpi">150</field>
  </record>
</odoo>


